{% trans_default_domain 'FOSUserBundle' %}

{% block form %}
    {{ form_start(form, {action: path('fos_user_resetting_reset', {token: token}), attr: {class: 'fos_user_resetting_reset'}}) }}
        {% block formFields %}
            {{ form_widget(form) }}
            <button class="btn btn-primary">{{ 'resetting.reset.submit'|trans }}</button>
        {% endblock %}
    {{ form_end(form) }}
{% endblock %}
