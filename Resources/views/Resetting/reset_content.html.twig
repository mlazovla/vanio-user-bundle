{% trans_default_domain 'FOSUserBundle' %}

{% block form %}
    {{ form_start(form, {attr: formAttributes ?? {class: 'fos_user_resetting_reset'}}) }}
        {% block formFields %}
            {{ form_widget(form) }}
            <div class="form-buttons form-group">
                <button class="btn btn-primary">{{ 'resetting.reset.submit'|trans }}</button>
            </div>
        {% endblock %}
    {{ form_end(form) }}
{% endblock %}
